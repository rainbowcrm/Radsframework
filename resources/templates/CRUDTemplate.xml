<?xml version="1.0" encoding="UTF-8"?>
<Template id ="simplecrud" type = "com.techtrade.rads.framework.ui.templates.CRUDTemplateType" >
 <DataType   > ModelObject
 </DataType>
 <Renderers>
 <Renderer>
 <RendererType>HTML</RendererType>
 <StyleSheet>./css/display.css</StyleSheet>
  <PageStyle>pagestyle</PageStyle>
 </Renderer>
 </Renderers>
 <fixedActionField>hdnFixedAction</fixedActionField>
 <SubmitActionField>submitAction</SubmitActionField>
 <FixedActionParamField>hdnFixedActionParam</FixedActionParamField>
 <MandatoryFieldMarker>*</MandatoryFieldMarker>
 <Errors showin ="detail"  style ="errorStyle" showinTable = "true" tabStyle ="errorStyle">
   <OddRow>oddRowError</OddRow>
   <EvenRow>evenRowError</EvenRow>
   <TitleRow>listPageTitleError</TitleRow>
 </Errors>
 <Sections>
 <Section Id ="leftsideBar"  style ="leftnav"  >
  <Panel key ="menuPanel"  Id ="leftnav" > </Panel>  
 </Section>   
 <Section Id ="header" style ="headerStyle"  showinTable = "true" >
   <noRows>-1</noRows>
  <noCols>3</noCols> 
  </Section>
 <Section Id ="detail" showinTable = "true"  style= "detailCRUDStyle"  tabStyle ="CRUDtableContainer">
  <noRows>-1</noRows>
  <noCols>2</noCols>
 </Section>
 <Section Id ="summary" showinTable = "true"  style= "summaryStyle" >
 <noRows>1</noRows>
 <noCols>1</noCols> 
 <Panel key ="CRUDPanel"  Id ="crudPanel" > </Panel>  
 </Section>
 </Sections>
 
 
 
 
